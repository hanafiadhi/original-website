{
  "compilerOptions": {
    "module": "ES2020",
    "target": "ES2020",
    "outDir": "./dist",
    "rootDir": "./src",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
  },
  "include": ["src/**/*"],
  "exclude": [
  "dist",
  "node_modules"
]
}
